{
  "name": "fixit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client": "react-scripts start",
    "server": "cd server && nodemon server.js",
    "start": "concurrently --killall -9 node \"npm run server\" \"npm run client\"",
    "preinstall": "rm -rf node_modules && rm -rf package-lock.json"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.3",
    "path": "^0.12.7",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "3.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
